bindsym $winKeyL+Left focus left
bindsym $winKeyL+Down focus down
bindsym $winKeyL+Up focus up
bindsym $winKeyL+Right focus right

# move focused window
bindsym $winKeyL+Shift+j move left
bindsym $winKeyL+Shift+k move down
bindsym $winKeyL+Shift+l move up
bindsym $winKeyL+Shift+semicolon move right

# alternatively, you can use the cursor keys:
bindsym $winKeyL+Shift+Left move left
bindsym $winKeyL+Shift+Down move down
bindsym $winKeyL+Shift+Up move up
