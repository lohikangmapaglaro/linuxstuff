#Gaps!
set $mode_gaps  Outer gap |   Inner gap
set $mode_gaps_outer  gap[]+ |   gap[]- |  [R]eset |  gap[^]+ |   gap[^]- |  [^R]eset 
set $mode_gaps_inner  gap[]+ |   gap[]- |  [R]eset |  gap[^]+ |   gap[^]- |  [^R]eset 

gaps inner 15
gaps outer 2
smart_gaps on
smart_borders no_gaps

bindsym $winKeyL+control+g mode "$mode_gaps"
mode "$mode_gaps" {
        bindsym o      mode "$mode_gaps_outer"
        bindsym i      mode "$mode_gaps_inner"
        bindsym Return $return
        bindsym Escape $return
}

mode "$mode_gaps_inner" {
        bindsym plus  gaps inner current minus 5
        bindsym minus gaps inner current plus 5
        bindsym r     gaps inner current set 0

        bindsym Shift+plus  gaps inner all minus 5
        bindsym Shift+minus gaps inner all plus 5
        bindsym Shift+r     gaps inner all set 0

        bindsym Return $return
        bindsym Escape $return
}
mode "$mode_gaps_outer" {
        bindsym plus  gaps outer current minus 5
        bindsym minus gaps outer current plus 5
        bindsym 0     gaps outer current set 0

        bindsym Shift+plus  gaps outer all minus 5
        bindsym Shift+minus gaps outer all plus 5
        bindsym Shift+0     gaps outer all set 0

        bindsym Return $return
        bindsym Escape $return
}

#Simple  power menu
set $Locker i3lock && sleep 1

set $mode_system   Lock |  Exit |  Suspend |  Hibernate |   Reboot |   shUtdown
mode "$mode_system" {
    bindsym l exec --no-startup-id $Locker, mode "default"
    bindsym e exec --no-startup-id i3-msg exit, mode "default"
    bindsym s exec --no-startup-id $Locker && systemctl suspend, mode "default"
    bindsym h exec --no-startup-id $Locker && systemctl hibernate, mode "default"
    bindsym r exec --no-startup-id systemctl reboot, mode "default"
    bindsym u exec --no-startup-id systemctl poweroff -i, mode "default"

   bindsym control+$altL+Delete  $return
   bindsym control+$altR+Delete $return
    bindsym Return $return
    bindsym Escape $return
}
bindsym control+$altL+Delete mode "$mode_system"
bindsym control+$altR+Delete mode "$mode_system"
